  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.25);
  position: relative;
  margin-top: 30px;
}

.box image {
  position: absolute;
  width: 57px;
  height: 57px;
  top: 7px;
  left: 17px;
}

.box text {
  position: absolute;
  line-height: 71px;
  left: 85px;
  font-size: 18px;
  font-weight: 400;
}

