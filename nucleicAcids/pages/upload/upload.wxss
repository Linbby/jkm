/* pages/sample/sample.wxss */
.now {
  position: absolute;
  top: 50px;
  right: 46px;
  font-size: 18px;
  font-weight: 400;
  letter-spacing: 0px;
  line-height: 24.7px;
  color: rgba(51, 51, 51, 1);
}

.box image.id {
  height: 39px;
  width: 51px;
  top: 15px;
}

.kind {
  font-size: 18px;
  font-weight: 400;
  letter-spacing: 0px;
  line-height: 24.7px;
  color: rgba(51, 51, 51, 1);
  margin-bottom: 32px;
}

.box {

  height: 71px;
  opacity: 1;
  border-radius: 11px;
  background: #ffffff;
  border: 1px solid rgba(166, 166, 166, 1);
  box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.25);
  position: relative;
  margin-top: 30px;
}

.box image {
  position: absolute;
  width: 57px;
  height: 57px;
  top: 7px;
  left: 17px;
}

.box text {
  position: absolute;
  line-height: 71px;
  left: 85px;
  font-size: 18px;
  font-weight: 400;
}

.module {
  padding-top: 200px;
}

.form {
  position: relative;
  top: -150px;
  height: 365px;
  opacity: 1;
  border-radius: 10px;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 6px rgba(55, 55, 55, 0.32);
  padding:16px;
}